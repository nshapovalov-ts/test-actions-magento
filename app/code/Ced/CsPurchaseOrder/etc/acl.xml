<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Ced_CsPurchaseOrder::cspurchaseorder" title="Categorywise Request For Quote"
                          translate="title" sortOrder="30">
                    <resource id="Ced_CsPurchaseOrder::purchaseorder_details" title="Quotes" sortOrder="100"/>
                    <resource id="Ced_CsPurchaseOrder::vendor_categories" title="Vendor Assigned Categories"
                              sortOrder="100"/>
                    <resource id="Ced_CsPurchaseOrder::configuration" title="Configuration" sortOrder="10">
                        <resource id="Ced_CsPurchaseOrder::purchaseorder_configuration" title="Configuration"
                                  sortOrder="100"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
